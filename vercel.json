{
  "version": 2,
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "docs.authzed.com"
        }
      ],
      "destination": "https://authzed.com/docs/",
      "permanent": true
    },
    {
      "source": "/:match*",
      "has": [
        {
          "type": "host",
          "value": "docs.authzed.com"
        }
      ],
      "destination": "https://authzed.com/docs/:match*",
      "permanent": true
    },
    {
      "source": "/:match*/",
      "has": [
        {
          "type": "host",
          "value": "docs.authzed.com"
        }
      ],
      "destination": "https://authzed.com/docs/:match*",
      "permanent": true
    }
  ]
}
